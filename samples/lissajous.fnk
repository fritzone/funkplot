set palette firestrm
set pixel size 5

var pssin pscos list of points
var i number
var x y number
var ps pc p point
let i = 0
function ps(x) = 3 * sin(3 * x + 3.14/2)
function pc(x) = 3 * sin(4 * x)

let pssin = points of ps over (-3.14, 3.14) counts 256
let pscos = points of pc over (-3.14, 3.14) counts 256

for i = 0 to 255 do

  let ps = pssin[i]
  let pc = pscos[i]

  let x = ps.y
  let y = pc.y

  let p = point at (x,y)
  set color palette[i]
  plot p

done
